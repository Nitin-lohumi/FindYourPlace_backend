 // output   = "../src/generated/prisma"
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id        String   @id @map("_id")  
  name      String
  email     String   @unique
  image     String?
  createdAt DateTime @default(now())
  savedPlace String[]
}

model SavedPlace {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  placeId   String
  photoUrl  String?
  placeData Json
  userIds   String[]
  createdAt DateTime @default(now())
}